{
	"extends": "airbnb",
  "rules": {
    "no-console": 0,
    "global-require": 0,
    "react/no-danger": 0,
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": [".js", ".jsx"]
      }
    }
  },
  "env": {
    "browser": true
  },
  "globals": {
    "$": true,
    "jQuery": true,
    "foundation": true
  }
}